g_pkg_flg = -bs-package-name your-project-name -bs-ns YourProjectName
src_root_dir = /app
bsc = /rbd/pnpm-volume/48efd6af-fb62-40dd-b041-becc024da94c/node_modules/.registry.npmjs.org/bs-platform/8.2.0/node_modules/bs-platform/linux/bsc.exe
bsdep = /rbd/pnpm-volume/48efd6af-fb62-40dd-b041-becc024da94c/node_modules/.registry.npmjs.org/bs-platform/8.2.0/node_modules/bs-platform/linux/bsb_helper.exe
warnings = 
bsc_flags = -bs-super-errors
ppx_flags = 
g_dpkg_incls = 
g_ns = -bs-ns YourProjectName
g_lib_incls = -I . -I src/lib -I src/backend -I src/ -I /app/node_modules/reason-react/lib/ocaml
rule build_ast_from_re
  command = $bsc  $warnings -bs-jsx 3 $bsc_flags -o $out -bs-syntax-only -bs-binary-ast $in
  description = [34mBuilding[39m [2m${out}[22m
build  src/lib/game.reast : build_ast_from_re $src_root_dir/src/lib/game.re
rule mk_deps
  command = $bsdep -hash cfa3dee32937418111adca3ce110622f $g_ns $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/lib/game.d : mk_deps src/lib/game.reast
rule ml_cmj_cmi
  command = $bsc $g_pkg_flg -bs-suffix $g_lib_incls $warnings $bsc_flags -o $out $in
  dyndep = $in_e.d
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  src/lib/game-YourProjectName.cmj |  src/lib/game-YourProjectName.cmi $src_root_dir/src/lib/game.bs.js : ml_cmj_cmi src/lib/game.reast ||  src/lib/game.d
  g_pkg_flg = $g_pkg_flg  -bs-package-output commonjs:src/lib
rule build_package
  command = $bsc -w -49 -color always -no-alias-deps  $in
  restat = 1
  description = [34mBuilding[39m [2m${out}[22m
build  YourProjectName.cmi : build_package YourProjectName.mlmap
